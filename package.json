{
  "name": "pub-sub",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./server/bin/www.js",
    "server": "node ./dist-server/bin/www",
    "dev": "NODE_ENV=development npm run build && npm run server",
    "transpile": "babel ./server --out-dir dist-server",
    "clean": "rimraf dist-server",
    "build": "npm run clean && npm run transpile",
    "watch:dev": "nodemon"
  },
  "dependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.7",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "rimraf": "^3.0.2",
    "sequelize": "^6.4.0",
    "sequelize-cli": "^6.2.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  }
}
